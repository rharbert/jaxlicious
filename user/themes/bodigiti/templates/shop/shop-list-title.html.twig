{% set title_level = title_level ?: 'h2' %}
{% if page.header.link %}
    <{{ title_level }} class="p-name mt-1">
        {% if page.header.continue_link is not sameas(false) %}
        {{ page.title }}
        {% endif %}
        {{ page.title }}
    </{{ title_level }}>
{% else %}
    <{{ title_level }} class="p-name mt-1">{{ page.title }}</{{ title_level }}>
{% endif %}
